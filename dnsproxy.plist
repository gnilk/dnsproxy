<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>se.nagini.dnsproxy</string>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>Sockets</key>
    <dict>
        <key>Listeners</key>
        <dict>
            <key>SockServiceName</key>
            <string>domain</string>
            <key>SockType</key>
            <string>stream</string>
            <key>SockFamily</key>
            <string>IPv4</string>
        </dict>
    </dict>
    <key>StandardOutPath</key>
    <string>/var/log/dnsproxy.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/dnsproxy.error.log</string>
    <key>Debug</key>
    <true/>


    <key>Program</key>
    <string>/Users/gnilk/src/go/src/dnsproxy/dnsproxy</string>


    <key>EnvironmentVariables</key> 
    <dict> 
        <key>PATH</key> 
        <string>/bin:/usr/bin:/usr/local/bin:/usr/local/go/bin</string> 
        <key>GOPATH</key>
        <string>/Users/gnilk/src/go</string>

    </dict>    

    <key>WorkingDirectory</key> 
    <string>/Users/gnilk/src/go/src/dnsproxy</string>    

</dict>
</plist>

